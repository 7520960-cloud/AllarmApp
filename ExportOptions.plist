<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- Тип экспорта: для публикации в App Store / TestFlight -->
    <key>method</key>
    <string>app-store</string>

    <!-- Team ID -->
    <key>teamID</key>
    <string>CW6VS28729</string>

    <!-- Включаем управление профилями автоматически -->
    <key>signingStyle</key>
    <string>automatic</string>

    <!-- Указываем provisioning profile -->
    <key>provisioningProfiles</key>
    <dict>
        <key>com.serhiitkachuk.AlarmApp</key>
        <string>alarmapp_appstore_profile</string>
    </dict>

    <!-- Указываем bundle identifier -->
    <key>bundleIdentifier</key>
    <string>com.serhiitkachuk.AlarmApp</string>

    <!-- Upload symbols для dSYM -->
    <key>uploadSymbols</key>
    <true/>

    <!-- Manage app thinning -->
    <key>stripSwiftSymbols</key>
    <true/>

    <!-- Manage bitcode -->
    <key>compileBitcode</key>
    <false/>
</dict>
</plist>
